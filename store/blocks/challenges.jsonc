    "props": {
      "roles": [
        "store-admin",
        "sales-admin",
        "sales-manager",
        "sales-representative",
        "customer-admin",
        "customer-approver",
        "customer-buyer"
      ]
    },
    "blocks": [
      "allowed-content#reseller",
      "disallowed-content#reseller"
    ]
  },
  "allowed-content#login": {
    "children": [
      "link#profile"
    ],
    "props": {
      "blockClass": "loginChallengeContent"
    }
